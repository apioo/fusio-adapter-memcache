{
  "name": "fusio/adapter-memcache",
  "description": "Adapter which can cache an action response using memcache",
  "homepage": "http://fusio-project.org",
  "keywords": ["fusio-adapter"],
  "license": "AGPL-3.0",
  "authors": [
    {
      "name": "Christoph Kappestein",
      "email": "christoph.kappestein@gmail.com",
      "homepage": "http://christoph.kappestein.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-memcached": ">=2.0",
    "doctrine/cache": "^1.6",
    "fusio/engine": "^0.4@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.1"
  },
  "autoload": {
    "psr-4": {
      "Fusio\\Adapter\\Memcache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Fusio\\Adapter\\Memcache\\Tests\\": "tests/"
    }
  }
}
